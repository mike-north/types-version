{
  "name": "types-version",
  "version": "0.0.0-development",
  "description": "Utilities for managing versions of TypeScript ambient types",
  "main": "dist/commonjs/src/index.js",
  "module": "dist/modules/src/index.js",
  "types": "dist/types/src/index.d.ts",
  "ember-addon": {
    "main": "ember-addon.js"
  },
  "keywords": [
    "ember-addon",
    "typescript"
  ],
  "bin": "./bin/types-version",
  "scripts": {
    "start": "node -r ts-node/register ./src/cli/index.ts",
    "problems": "node ./scripts/problems.js",
    "preversion": "npm run test",
    "prepare": "ember build -prod",
    "build": "ember build -prod",
    "build-tests": "ember build",
    "pretest": "npm run problems",
    "test": "ember test",
    "tdd": "ember test --server",
    "travis-deploy-once": "travis-deploy-once",
    "semantic-release": "semantic-release"
  },
  "license": "MIT",
  "devDependencies": {
    "@mike-north/js-lib-semantic-release-config": "^0.0.0-development",
    "@types/commander": "^2.12.2",
    "@types/find-up": "^2.1.1",
    "@types/inquirer": "^0.0.43",
    "@types/node": "^10.9.1",
    "@types/qunit": "^2.0.31",
    "@types/semver": "^5.5.0",
    "ember-cli": "^3.1.4",
    "libkit": "~0.6.0",
    "qunit": "^2.4.1",
    "semantic-release": "^15.9.9",
    "shelljs": "^0.7.8",
    "travis-deploy-once": "^5.0.3",
    "ts-node": "^7.0.1",
    "tslint": "^5.7.0",
    "typescript": "^3.0.1"
  },
  "dependencies": {
    "commander": "^2.17.1",
    "exit": "^0.1.2",
    "find-up": "^3.0.0",
    "pkg-up": "^2.0.0"
  },
  "toolchain": {
    "node": "10.9.0",
    "yarn": "1.9.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mike-north/types-version.git"
  },
  "release": {
    "extends": "@mike-north/js-lib-semantic-release-config"
  }
}
